<script>
function funcNav()
{
	var div2=document.getElementById('div2');
	if(div2.style.width=="250px")
		div2.style.width="50px";
	else
		div2.style.width="250px";
}
var yesbtn = document.getElementById('yesbtn');
yesbtn.addEventListener("click" , function(abc)
{
window.location.href="http://localhost:8000/linkedin";
})
var nobtn = document.getElementById('nobtn')
nobtn.addEventListener("click" , function(abc)
{
	  var modal10 = document.getElementById('myModal');
		modal10.style.display = "none";
})
var modal20 = document.getElementById('myModal2');
var span1 = document.getElementsByClassName("close2")[0];
span1.onclick = function() {
modal20.style.display = "none";
}
var snobtn = document.getElementById('snobtn')
snobtn.addEventListener("click" , function(abc)
{
	  var modal20 = document.getElementById('myModal2');
		modal20.style.display = "none";
})
var syesbtn = document.getElementById('syesbtn');
syesbtn.addEventListener("click" , function(abc)
{
window.location.href="http://localhost:8000/ASUdetails";
})
var switchuser = document.getElementById('switchspan');
switchuser.addEventListener("click" , function(abc)
{
  var modal = document.getElementById('myModal2');
  var span = document.getElementsByClassName("close")[0];
  modal.style.display = "block";
  span.onclick = function()
{
  modal.style.display = "none";
}
})
</script>
